<?xml version="1.0" encoding="utf-8"?>
<com.jakchang.viewdrawing.MyLinearLayoutR xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    tools:context=".MainActivity">

    <com.jakchang.viewdrawing.MyLinearLayout1
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <com.jakchang.viewdrawing.MyTextView1
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Hello World!" />

        <com.jakchang.viewdrawing.MyTextView2
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Hello World!" />
    </com.jakchang.viewdrawing.MyLinearLayout1>

    <com.jakchang.viewdrawing.MyLinearLayout2
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <com.jakchang.viewdrawing.MyTextView3
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Hello World!" />
        <com.jakchang.viewdrawing.MyTextView4
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Hello World!" />
    </com.jakchang.viewdrawing.MyLinearLayout2>

</com.jakchang.viewdrawing.MyLinearLayoutR>